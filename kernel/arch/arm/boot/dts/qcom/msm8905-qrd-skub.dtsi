#include "msm8909-qrd.dtsi"
#include "msm8905.dtsi"
#include "msm8905-camera-sensor-skub.dtsi"
#include "msm8905-pm8909.dtsi"

&soc {
	matrix_keypad: matrix_keypad@0 {
		compatible = "gpio-matrix-keypad";
		input-name = "matrix_keypad";
		debounce-delay-ms = <10>;
		col-scan-delay-us = <7>;
		pinctrl-names = "tlmm_gpio_key_active","tlmm_gpio_key_suspend";
		pinctrl-0 = <&gpio_key_active>;
		pinctrl-1 = <&gpio_key_suspend>;

		row-gpios = <&msm_gpio 97 0
			&msm_gpio 96 0
			&msm_gpio 95 0
			&msm_gpio 94 0>;

		col-gpios = <&msm_gpio 52 0
			&msm_gpio 56 0
			&msm_gpio 7 0
			&msm_gpio 99 0
			&msm_gpio 6 0>;
		linux,keymap = <0x000000e7      /* (0,0) KEY_DIAL */
       		0x00010002      /* (1,0) KEY_1 */
       		0x00020005      /* (2,0) KEY_4 */
       		0x00030008      /* (3,0) KEY_7 */
       		0x0004020a      /* (4,0) KEY_NUMERIC_STAR */

			0x01000069      /* (0,1) KEY_LEFT */
			0x01010003      /* (1,1) KEY_2 */
			0x01020006      /* (2,1) KEY_5 */
			0x01030009      /* (3,1) KEY_8 */
			0x0104000b      /* (4,1) KEY_0 */

			0x0200008b      /* (0,2) KEY_MENU */
       		0x02010004      /* (1,2) KEY_3 */
       		0x02020007      /* (2,2) KEY_6 */
       		0x0203000A      /* (3,2) KEY_9 */
       		0x0204020b      /* (4,2) KEY_NUMERIC_POUND */

			0x03000160      /* (0,3) KEY_OK */
			0x0301006A      /* (1,3) KEY_RIGHT */
			0x0302009e>;      /* (3,3) KEY_BACK */
	};

	gpio_keys {
		compatible = "gpio-keys";
		input-name = "gpio-keys";
		pinctrl-names = "tlmm_gpio_key_active","tlmm_gpio_key_suspend";
		pinctrl-0 = <&gpio_key_active>;
		pinctrl-1 = <&gpio_key_suspend>;

		vol_up {
			label = "volume_up";
			gpios = <&msm_gpio 90 0x1>;
			linux,input-type = <1>;
			linux,code = <103>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};

		end_key {
			label = "end_key";
			gpios = <&msm_gpio 98 0x1>;
			linux,input-type = <1>;
			linux,code = <116>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};

};

&tlmm_pinmux {
	tlmm_gpio_key {
		qcom,pins = <&gp 52>, <&gp 56>, <&gp 7>, <&gp 99>, <&gp 6>, <&gp 98>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <6>;
		label = "tlmm_gpio_key";
		gpio_key_active: gpio_key_active {
			drive-strength = <2>;
			bias-pull-up;
		};
		gpio_key_suspend: gpio_key_suspend {
			drive-strength = <2>;
			bias-pull-up;
		};
	};

	hall_sensor0_int_pin {
		qcom,pins = <&gp 92>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "hall_sensor0-irq";
		hall_sensor0_int_default: hall_sensor0_int_default {
			drive-strength = <6>;
			bias-pull-up;
		};
	};

	hall_sensor1_int_pin {
		qcom,pins = <&gp 36>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "hall_sensor1-irq";
		hall_sensor1_int_default: hall_sensor1_int_default {
			drive-strength = <6>;
			bias-pull-up;
		};
	};
};

